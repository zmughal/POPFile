        <td>
        </td>
        <td align="right">
            <TMPL_VAR NAME="Bucket_Percentage">
        </td>
        <td>
        </td>
    </tr>
</table>

<form action="/buckets" method="post">
    <input type="hidden" name="session" value="<TMPL_VAR NAME="Session_Key">" />
    <input type="hidden" name="showbucket" value="<TMPL_VAR NAME="Bucket_Bucket_Name">" />
    <input type="submit" name="clearbucket" value="<TMPL_VAR NAME="Localize_SingleBucket_ClearBucket">" />
</form>

<TMPL_IF NAME="Bucket_If_Has_Words">

